<dynamic_object>
	<diesel materials="units/pd2_dlc_holly/river/vehicles/anim_vehicle_rubber_boat/anim_vehicle_rubber_boat" orientation_object="rp_anim_boat" />
	<sequence_manager file="units/pd2_dlc_holly/river/vehicles/anim_vehicle_rubber_boat/anim_vehicle_rubber_boat" />
	<animation_def name="anims/units/rubber_boat/rubber_boat" />

	<bodies>
		<body name="body_boat" enabled="true" template="animated">
			<object name="anim_body" />
			<object name="c_mesh_mopp_01" collision_type="mesh_mopp" />
		</body>
		<body name="body_motor" enabled="true" template="animated">
			<object name="anim_motor" />
			<object name="c_mesh_mopp_motor" collision_type="mesh_mopp" />
		</body>
	</bodies>
	<decal_surfaces default_material="rubber">
		<decal_mesh name="dm_plastic"					enabled="true" material="plastic" />
		<decal_mesh name="dm_rubber"					enabled="true" material="rubber" />
		<decal_mesh name="dm_sheet_metal"				enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_sheet_metal_motor"			enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_sheet_metal_propeller"		enabled="true" material="sheet_metal" />
	</decal_surfaces>
	<graphics>
		<lod_object name="lod_g">
			<object name="g_lod0" enabled="true" max_distance="1500" />
			<object name="g_lod1" enabled="true" max_distance="3000" />
			<object name="g_lod2" enabled="true" />
		</lod_object>
		<object name="g_head"	enabled="true" />
		<object name="g_body"	enabled="true" />
		<object name="g_hands"	enabled="true" />
		<object name="s_s"		enabled="true" shadow_caster="true" />

		<object name="g_lootbag_01" enabled="false"/>
		<object name="g_lootbag_02" enabled="false"/>
		<object name="g_lootbag_03" enabled="false"/>
		<object name="g_lootbag_04" enabled="false"/>
		<object name="g_lootbag_05" enabled="false"/>
		<object name="g_lootbag_06" enabled="false"/>
		<object name="g_lootbag_07" enabled="false"/>
		<object name="g_lootbag_08" enabled="false"/>
		<object name="g_lootbag_09" enabled="false"/>
		<object name="g_lootbag_10" enabled="false"/>
		<object name="g_lootbag_11" enabled="false"/>
		<object name="g_lootbag_12" enabled="false"/>
		<object name="g_lootbag_13" enabled="false"/>
		<object name="g_lootbag_14" enabled="false"/>
		<object name="g_lootbag_15" enabled="false"/>
		<object name="g_lootbag_16" enabled="false"/>
		<object name="g_lootbag_17" enabled="false"/>
		<object name="g_lootbag_18" enabled="false"/>
		<object name="g_lootbag_19" enabled="false"/>
		<object name="g_lootbag_20" enabled="false"/>
	</graphics>
</dynamic_object>
